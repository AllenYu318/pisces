EXAMPLE = collector.sup base.sup ext_base.sup emitter.sup example3.out

all::

# Build a Release

release:: Please Makefile Makefile.dep Makefile
	$(RMDIR) $(RELEASEDIR)
	$(MKDIR) $(RELEASEDIR)
	$(CHMOD) $(INSTDIRFLAGS) $(RELEASEDIR)

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in Makefile.dep Makefile Imakefile ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

clean::
	$(RMDIR) $(RELEASEDIR)

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in Imakefile Makefile ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in $(EXAMPLE) ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

