	xtimer_bsd.f xtimer_def.f xtimer_sgi.c xtimer_vms.for

# -----------------------------------------------------------------------------

all:: $(LIB) $(TESTPROG)

# I. Make the program.

$(TESTPROG):  $(MAIN)  $(LIB)
	$(RM) $@
	$(F77) -o $@ $(FLDFLAGS)  $(MAIN)  $(LIB)   $(SYSLIBS)
	@echo make $(TESTPROG) done.

clean::
	$(RM) $(TESTPROG)

# II. Make the library.

$(LIB):  $(OBJS)
	$(RM) $@
	$(AR) $(LIB)  $(OBJS)
	$(RANLIB) $(RANLIBFLAGS) $(LIB)
	@echo make $(LIB) done.

# III. Run the test script

runtst: $(TESTPROG)
	runtst$(SHELL_EXT)

install:: $(SYSF77)
$(SYSF77): $(LIB)
	$(INSTALL) $(INSTLIBFLAGS) $(LIB) $(SYSF77)
	$(RANLIB) $(RANLIBINSTFLAGS) $(SYSF77)

