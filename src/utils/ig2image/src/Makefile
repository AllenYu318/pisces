S4DIR = ../s4stuff
S4LIB = libs4.a

DEST	= ig2image

INCFILES = bound.h more_imp.h

INCLUDES = -I$(S4DIR) -I.

CSOURCES = main.c \
	fill_grid.c make_grid.c get_graphsize.c \
	write_imagetool.c newget_solval.c window.c mylog.c \
	makeframe.c incfil.c s4incf.c \
	scan_s4file.c

SOURCES = $(CSOURCES) readp2.f

OBJECTS	= main.o fill_grid.o make_grid.o get_graphsize.o \
	write_imagetool.o newget_solval.o window.o mylog.o \
	makeframe.o readp2.o incfil.o s4incf.o \
	scan_s4file.o

all:: $(DEST)

$(DEST): $(OBJECTS) $(S4DIR)/$(S4LIB)
	$(RM) $@
	$(CC) -o $@ $(CLDFLAGS) $(OBJECTS) $(S4DIR)/$(S4LIB) $(F77LIBS) $(C_TOOLLIBS) $(SYSLIBS)
	@echo make $(DEST) done.

clean::
	$(RM) $(DEST)

install:: $(DINDIR)/$(DEST)
$(DINDIR)/$(DEST): $(DEST)
	$(INSTALL) $(INSTBINFLAGS) $(DEST) $(DINDIR)/$(DEST)

install:: $(MANDIR)/$(DEST).$(MANEXT)
$(MANDIR)/$(DEST).$(MANEXT): $(DEST).man
	$(INSTALL) $(INSTMANFLAGS) $(DEST).man $(MANDIR)/$(DEST).$(MANEXT)

# Build a Release

release:: Please Makefile Makefile.dep $(DEST).cat
	$(RMDIR) $(RELEASEDIR)
	$(MKDIR) $(RELEASEDIR)
	$(CHMOD) $(INSTDIRFLAGS) $(RELEASEDIR)

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in Makefile.dep Makefile Imakefile ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

clean::
	$(RMDIR) $(RELEASEDIR)

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in $(SOURCES) $(INCFILES) ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in $(DEST).man $(DEST).cat ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

# build the cat'able version of the manual

$(DEST).cat: $(DEST).man
	$(NROFF) $(MANFLAGS) $(DEST).man $(NROFF_FILTER) > $(DEST).cat

###########################################################################
# Imake.tmpl common rules for all Makefiles - do not edit

emptyrule::

clean::
	$(RM_CMD)

Makefile:: $(ICONFIGFILES)
	-@if [ -f Makefile ] ; then \
	$(RM) Makefile.bak; $(CP) Makefile Makefile.bak;\
	fi
	$(IMAKE_CMD)

depend::
	$(MAKEDEP) $(MAKEDEP_FLAGS) $(SOURCES) $(MAKEDEP_OUTPUT)

lint::
	$(LINT) $(LINTFLAGS) $(CSOURCES) $(LINTLIBS)

release::
	-(cd $(RELEASEDIR) ; make Makefile depend; \
	touch Imakefile; make Makefile)

release::
	@echo "release done."

Makefiles::
	@echo "Makefiles done."
install::
	@echo "install done."
lintlibs::
	@echo "lintlibs done."

