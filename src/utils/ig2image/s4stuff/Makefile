all:: $(LIBNAME)

$(LIBNAME):  $(OBJECTS)
	$(RM) $@
	$(AR) $(LIBNAME)  $(OBJECTS)
	$(RANLIB) $(RANLIBFLAGS) $(LIBNAME)
	@echo make $(LIBNAME) done.

install::  $(LIBDIR)/$(DEST)
$(LIBDIR)/$(DEST): $(DEST)
	$(INSTALL) $(INSTLIBFLAGS) $(DEST)  $(LIBDIR)/$(DEST)
	$(RANLIB) $(RANLIBINSTFLAGS)  $(LIBDIR)/$(DEST)

# Build a Release

release:: Please Makefile Makefile.dep $(LIBNAME)
	$(RMDIR) $(RELEASEDIR)
	$(MKDIR) $(RELEASEDIR)
	$(CHMOD) $(INSTDIRFLAGS) $(RELEASEDIR)

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in Makefile.dep Makefile Imakefile ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

clean::
	$(RMDIR) $(RELEASEDIR)

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in $(SOURCES) $(INCFILES) ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

###########################################################################
# Imake.tmpl common rules for all Makefiles - do not edit

emptyrule::

clean::
	$(RM_CMD)

Makefile:: $(ICONFIGFILES)
	-@if [ -f Makefile ] ; then \
	$(RM) Makefile.bak; $(CP) Makefile Makefile.bak;\
	fi
	$(IMAKE_CMD)

depend::
	$(MAKEDEP) $(MAKEDEP_FLAGS) $(SOURCES) $(MAKEDEP_OUTPUT)

lint::
	$(LINT) $(LINTFLAGS) $(CSOURCES) $(LINTLIBS)

release::
	-(cd $(RELEASEDIR) ; make Makefile depend; \
	touch Imakefile; make Makefile)

release::
	@echo "release done."

Makefiles::
	@echo "Makefiles done."
install::
	@echo "install done."
lintlibs::
	@echo "lintlibs done."

