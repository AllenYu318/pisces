.c.o:
	$(CC) $(CMINUS_C) $(CFLAGS) $*.c

.F.o:
	$(F77) $(FMINUS_C) $(FFLAGS) $*.F

.r.o:
	$(RATFOR) $(RFLAGS) $*.r > $*.f
	$(F77) $(FMINUS_C) $(FFLAGS) $*.f

