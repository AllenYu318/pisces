#...Directories where things are borrowed from.
DGEN    = ../misc
DSKEL   = ../skel
DSPARSE = ../sparse
DINCL   = .
DPLOT	= /usr/include/local

#...What we are borrowing - sparse matrix routines, low level graphics,
#...general and timing stuff.
GEN     = $(DGEN)/libmisc.a
SKEL    = $(DSKEL)/skelp.o $(DSKEL)/sys.o
SPARSE  = $(DSPARSE)/libsparse.a

#... uncomment the line below for the version of tri that is to
#...  	be used in the Polygon to Grid utility
#
# DEFINES = -D DONTSUBDIVIDE

INCLUDES = -I$(DINCL) -I$(DGEN) -I$(DSKEL) -I$(DSPARSE) -I$(DPLOT)

OTHER_LIBS = $(GEN) $(SKEL) $(SPARSE)
#
#...Triangulation and optimization code
#
DEST	= tri

INCFILES = dbase.h geom.h list.h

TRISRC	= main.c triang.c bfill.c ckmesh.c triheur.c tridata.c hinit.c \
	meshio.c dbase.c geom.c plot.c score.c

OPTSRC	=  matrix.c gn_opt.c laplace.c gn_asm.c list.c flip.c gn_func.c opt.c

SOURCES	= $(TRISRC) $(OPTSRC)

TRIOBJ	= main.o triang.o bfill.o ckmesh.o triheur.o tridata.o hinit.o \
	meshio.o dbase.o geom.o plot.o score.o

OPTOBJ	=  matrix.o gn_opt.o laplace.o gn_asm.o list.o flip.o gn_func.o opt.o

OBJECTS	= $(TRIOBJ) $(OPTOBJ)

all:: $(DEST)

$(DEST): $(OBJECTS) $(OTHER_LIBS)
	$(RM) $@
	$(CC) -o $@ $(CLDFLAGS) $(OBJECTS) $(OTHER_LIBS)   $(C_TOOLLIBS) $(SYSLIBS)
	@echo make $(DEST) done.

clean::
	$(RM) $(DEST)

install:: $(BINDIR)/$(DEST)
$(BINDIR)/$(DEST): $(DEST)
	$(INSTALL) $(INSTBINFLAGS) $(DEST) $(BINDIR)/$(DEST)

install:: $(MANDIR)/$(DEST).$(MANEXT)
$(MANDIR)/$(DEST).$(MANEXT): $(DEST).man
	$(INSTALL) $(INSTMANFLAGS) $(DEST).man $(MANDIR)/$(DEST).$(MANEXT)

# Build a Release

release:: Please Makefile Makefile.dep $(DEST).cat
	$(RMDIR) $(RELEASEDIR)
	$(MKDIR) $(RELEASEDIR)
	$(CHMOD) $(INSTDIRFLAGS) $(RELEASEDIR)

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in Makefile.dep Makefile Imakefile ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

clean::
	$(RMDIR) $(RELEASEDIR)

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in $(SOURCES) $(INCFILES) ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

release::
	@case '$(MFLAGS)' in *[ik]*) set +e;; esac ;\
	for f in $(DEST).man $(DEST).cat ; do \
	echo installing $(RELEASEDIR)/$$f ;\
	$(INSTALL) $(COPYFILEFLAGS) $$f $(RELEASEDIR)/$$f ;\
	done

# build the cat'able version of the manual

$(DEST).cat: $(DEST).man
	$(NROFF) $(MANFLAGS) $(DEST).man $(NROFF_FILTER) > $(DEST).cat

###########################################################################
# Imake.tmpl common rules for all Makefiles - do not edit

emptyrule::

clean::
	$(RM_CMD)

Makefile:: $(ICONFIGFILES)
	-@if [ -f Makefile ] ; then \
	$(RM) Makefile.bak; $(CP) Makefile Makefile.bak;\
	fi
	$(IMAKE_CMD)

depend::
	$(MAKEDEP) $(MAKEDEP_FLAGS) $(SOURCES) $(MAKEDEP_OUTPUT)

lint::
	$(LINT) $(LINTFLAGS) $(CSOURCES) $(LINTLIBS)

release::
	-(cd $(RELEASEDIR) ; make Makefile depend; \
	touch Imakefile; make Makefile)

release::
	@echo "release done."

Makefiles::
	@echo "Makefiles done."
install::
	@echo "install done."
lintlibs::
	@echo "lintlibs done."

