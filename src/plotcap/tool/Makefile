# Tue Jul 11 16:17:13 PDT 1989 (dredge--stanford)
#
# Make the various gplot tool programs.  These are connected to by pipes by
#	gplot() and contain the actual drive software of the particular
#	window system. The plotcap device description is given in tool.pc
#	and is the same for all devices.  The main (tool.c) takes the
#	plotcap infor coming from gplot() and calls a few simple routines
#	that may be implemented for the differnt tools (but need not be).
#
# Versions supported:
#	sunview	- for suns running sunview (and the notifier).
#	x10	- X10 protocol version.
#	x11	- X11 protocol version.
#
# written:  Michael Eldredge (dec 86)
# modified: MJE (feb) make lint
# modified: MJE (aug 88) added hsl2rgb utility
# modified: MJE (apr 89) added X11 version
# modified: MJE (jul 89) for port to MIPS box

# ------- Things easily changed ----------------------------------------------

# Development plotcap source directory -- to build the release the plotcap
#   entry must be put there....
PLOTCAP_SRC= ../plotcap

# ----------------------------------------------------------------------------

# Define for each tool its program name ( TOOL ), objects list ( TOOL_O ),
#   the libraries required ( TOOL_L ) and the installed destination ( TOOL_D )

# Sunview version
SUNVIEW	  = sunviewgplot
SUNVIEW_O = sunview_lib.o
SUNVIEW_L = -lsuntool -lsunwindow -lpixrect
SUNVIEW_D = ${LIBDIR}/${SUNVIEW}

# X(10) version.
X	= x10gplot
X_O	= x10_lib.o
X_L	= -lX
X_D	= ${LIBDIR}/${X}

# X11 version.
X11	= x11gplot
X11_O	= x11_lib.o
X11_L	= -lX11
X11_D	= ${LIBDIR}/${X11}

# ====== The Targets that this system will support. ========

all:: tool.o did.cnf

all:: $(X11)

did.cnf:
	$(RM) Makefile.cnf
	check$(SHELL_EXT) X10 $(X) $(X_L)
	check$(SHELL_EXT) X11 $(X11) $(X11_L)
	check$(SHELL_EXT) SUNVIEW $(SUNVIEW) $(SUNVIEW_L)
	touch $@
	touch Imakefile
	$(MAKE) Makefile
	$(MAKE)

clean::
	$(RM) did.cnf first first.*

# Sun View version -----------------------------------------------------------

$(SUNVIEW): tool.o $(SUNVIEW_O)
	$(RM) $@
	$(CC) -o $@ $(CLDFLAGS) tool.o $(SUNVIEW_O)    $(SUNVIEW_L) $(SYSLIBS)
	@echo make $(SUNVIEW) done.

clean::
	$(RM) $(SUNVIEW)

$(SUNVIEW_O):: sunview.icon

# X11 version -----------------------------------------------------------------

$(X11): tool.o $(X11_O)
	$(RM) $@
	$(CC) -o $@ $(CLDFLAGS) tool.o $(X11_O)    $(X11_L) $(SYSLIBS)
	@echo make $(X11) done.

clean::
	$(RM) $(X11)

$(X11_O):: x11.curs

# X10 version -----------------------------------------------------------------

$(X): tool.o $(X_O)
	$(RM) $@
	$(CC) -o $@ $(CLDFLAGS) tool.o $(X_O)    $(X_L) $(SYSLIBS)
	@echo make $(X) done.

clean::
	$(RM) $(X)

$(X_O):: x.icon

# make the icon from the sun-iconedit version (flip bits).
x.icon: sunview.icon sun2x
	./sun2x > x.icon

sun2x: sun2x.c
	$(RM) $@
	$(CC) -o $@ $(CLDFLAGS) sun2x.c    -DNEW_NAME=\"x\" $(SYSLIBS)
	@echo make sun2x done.

clean::
	$(RM) sun2x

# Misc progs ------------------------------------------------------------------

hsl2rgb: hsl2rgb.o
	$(RM) $@
	$(CC) -o $@ $(CLDFLAGS) hsl2rgb.o     $(SYSLIBS)
	@echo make hsl2rgb done.

clean::
	$(RM) hsl2rgb

hsl2rgb.o: hsl2rgb.c
	$(CC) $(CMINUS_C) $(CFLAGS) -DMAIN $*.c

# Misc Targets ================================================================

# Install each one.

install:: $(BINDIR)/hsl2rgb
$(BINDIR)/hsl2rgb: hsl2rgb
	$(INSTALL) $(INSTBINFLAGS) hsl2rgb $(BINDIR)/hsl2rgb

install:: $(X11_D)
$(X11_D): $(X11)
	$(INSTALL) $(INSTBINFLAGS) $(X11) $(X11_D)

###########################################################################
# Imake.tmpl common rules for all Makefiles - do not edit

emptyrule::

clean::
	$(RM_CMD)

Makefile:: $(ICONFIGFILES)
	-@if [ -f Makefile ] ; then \
	$(RM) Makefile.bak; $(CP) Makefile Makefile.bak;\
	fi
	$(IMAKE_CMD)

depend::
	$(MAKEDEP) $(MAKEDEP_FLAGS) $(SOURCES)

lint::
	$(LINT) $(LINTFLAGS) $(CSOURCES) $(LINTLIBS)

release::
	@echo "release done."

Makefiles::
	@echo "Makefiles done."
install::
	@echo "install done."
lintlibs::
	@echo "lintlibs done."

# Force any target -- make it depend on 'Please'
