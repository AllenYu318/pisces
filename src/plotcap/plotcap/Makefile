# Mon Sep 18 12:42:02 PDT 1989 (dredge--stanford)
#
# "Makefile": ``compile'' the plotcap data base from its many pieces.
#

HEAD	= Pc.head Pc.tail

P	= canon.pc dgl.pc ditroff.pc gpsave.pc \
	hp2563.pc hp2623.pc hp2648.pc hp2671.pc hp9872.pc hpthinkjet.pc \
	lw.pc movies.pc mskermit.pc pic.pc printronix.pc rlp.pc  \
	tek401x.pc tek4100.pc tek4510.pc \
	tool.pc tplot.pc tty.pc vt240.pc

all:: plotcap

install:: $(PLOTCAP).std
$(PLOTCAP).std: plotcap
	$(INSTALL) $(INSTFILEFLAGS) plotcap $(PLOTCAP).std

install:: $(PLOTCAP)
$(PLOTCAP): plotcap.def
	@-if test ! -f $(PLOTCAP) ; then \
	echo $(INSTALL) $(INSTFILEFLAGS) plotcap.def $(PLOTCAP) ; \
	$(INSTALL) $(INSTFILEFLAGS) plotcap.def $(PLOTCAP) ; \
	fi

plotcap.def::Makefile
	@echo '# Date:' > plotcap.def
	@echo '# LOCAL DEFINITIONS, edit this not $(PLOTCAPDIR)/plotcap.std' >>plotcap.def
	@echo ' ' >> plotcap.def
	@echo '%next "$(PLOTCAPDIR)/plotcap.std"' >>plotcap.def
	@echo ' ' >> plotcap.def
	@echo '# -------------------------------------------' >>plotcap.def
	@echo '# -- Add local defintions here --------------' >>plotcap.def
	@echo 'tty:	LIKE=hp2648::' >>plotcap.def

